{
  "name": "ang-kit",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json --port 6100",
    "build-package": "ng build --prod @ang-kit/http && ng build --prod @ang-kit/component && ng build --prod @ang-kit/auth && ng build --prod @ang-kit/util",
    "publish-auth-package": "cd dist\\ang-kit\\auth && npm publish -access public && cd ../../../",
    "publish-component-package": "cd dist\\ang-kit\\component && npm publish -access public && cd ../../../",
    "publish-http-package": "cd dist\\ang-kit\\http && npm publish -access public && cd ../../../",
    "publish-util-package": "cd dist\\ang-kit\\util && npm publish -access public && cd ../../../",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "npm publish -access public"
  },
  "author": "yang <704807396@qq.com>",
  "private": true,
  "dependencies": {
